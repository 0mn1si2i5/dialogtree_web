{
  "name": "DialogTree_web",
  "version": "1.0.0",
  "description": "DialogTree - 支持对话分叉的智能对话系统前端应用",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "axios": "^1.6.2",
    "d3": "^7.8.5",
    "dayjs": "^1.11.10",
    "md-editor-v3": "^5.8.3",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
